<!DOCTYPE html>
<html lang="en">

<head>
    <% include ./partials/head %>
</head>
<% include ./partials/script %>

    <body class="bgcol">
        <form action="/result" method="post" >
            <input type="hidden" name="psid" id="psid" value="<%=psid %>">
          
              <div class="container contact" id="one">
                <div class="row">
                    <div class="col-md-3 text-center pb-2">
                        <div class="contact-info">
                            <img src="/img/Fillinfo.png" alt="image" width="70px" height="70px"/>
                            <h3>Please Fill Your Information</h3>
                            <h4>Win Your Lucky Draw!</h4>
                        </div>
                    </div>
                    <div class="col-md-9 pb-2">
                        <div class="contact-form">
                            <div class="form-group">
                              <label class="control-label col-sm-2" for="name">Name:</label>
                              <div class="col-sm-10">          
                                <input type="text" class="form-control" id="fname" placeholder="Enter Your Name" name="name">
                              </div>
                            </div>
                            <div class="form-group">
                              <label class="control-label col-sm-2" for="age">Age:</label>
                              <div class="col-sm-10">          
                                <input type="text" class="form-control" id="age" placeholder="Enter your age" name="age">
                              </div>
                            </div>
                            
                            <div class="form-group">
                              <label class="control-label col-sm-2" for="phno">PhoneNumber:</label>
                              <div class="col-sm-10">
                                <input type="tel" class="form-control" id="phno" placeholder="Enter PhoneNumber" name="phno">
                              </div>
                            </div>

                            <div class="form-group">                              
                              <label class="control-label col-sm-2" for="gender">Gender:</label>
                              <div class="col-sm-10">
                              <select class="form-control" name="sex" id="gender">
                              <option value="male">Male</option>
                              <option value="female">Female</option>     
                            </select>
                          </div>
                          </div>

                          <div class="form-group">
                            <label class="control-label col-sm-2" for="stateNDiv">State And Division:</label>   
                            <div class="col-sm-10">                        
                            <select class="form-control" name="stateNDiv" id="stateNDiv">
                        <option selected>Select State and Division</option>
                        </select>
                      </div>
                        </div>

                        <div class="form-group">                            
                            <label class="control-label col-sm-2" for="township">Township</label>
                            <div class="col-sm-10">    
                            <select class="form-control" name="township" id="township">
                <option selected>Select Township</option>
                </select>
                        </div>
                        </div>
                            <div class="form-group">
                              <div class="col-sm-offset-2 col-sm-10"></div>
                              <button type="button" class="btn btn-outline-primary" id="nextBtn1" onclick='demoVisibilityone()'> Next  </button>
                          </div>
                          </div>

                        </div>
                    </div>
                </div>
            </div>


               


            <!--Second Page-->
            <div class="container" >

              <div class="container contact" id="two" style=" display : none">
                <div class="row">
                  <div class="col-md-3 text-center">
                    <div class="contact-info">
                        <img src="/img/Fillinfo.png" alt="image" width="70px" height="70px"/>
                        <h3>Please Fill Your Information</h3>
                        <h4>Win Your Lucky Draw!</h4>
                    </div>
                </div>
                    <div class="col-md-9">
                        <div class="contact-form w-100">
                            <label for="chooseproduct">Choose Your Product :</label>
        
                            <div class="row">
                                <div class="col-sm-6">Product Name:</div>
                                <div class="col-sm-6">
                                    <select class="form-control" id="myselection">
                                        <option>Select Option</option>
                                        <option value="One">SPY</option>
                                        <option value="Two">100 Plus</option>
                                        <option value="Three">Coca</option>
                                    </select>
                                </div>
                            </div>
        
                            <br>
                            <div id="showOne" class="myDiv">
                                <div class="row">
                                <div class="col-sm-6">Promotion Time:  </div>
                                <div class="col-sm-6">4 Feb 2020 to 11 March 2021</div>
                                </div>
        
                                <!--caoursel-->
                                <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
                                    <div class="carousel-inner">
                                      <div class="carousel-item active">
                                        <img src="/img/spy1.png" width="200" height="200" class="shadow-1-strong mb-4 mx-auto d-block" alt="SPY1">
                                      </div>
                                      <div class="carousel-item">
                                        <img src="/img/redspy.jpg" width="200" height="200" class="shadow-1-strong mb-4 mx-auto d-block" alt="SPY2">
                                      </div>
                                      <div class="carousel-item">
                                        <img src="/img/blackspy.jpg" width="200" height="200" class="shadow-1-strong mb-4 mx-auto d-block" alt="SPY3">
                                      </div>
                                    </div>
                                    <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
                                      <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                                      <span class="sr-only">Previous</span>
                                    </a>
                                    <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
                                      <span class="carousel-control-next-icon" aria-hidden="true"></span>
                                      <span class="sr-only">Next</span>
                                    </a>
                                  </div>
        
                                  <h5 class="text-center">SPY Wine Cooler</h5>
                                        <p class="text-center">has been the premier Ready To Drinkbrand among Thais for the past 30 years and has dominated the market with more than 70%, lead in both the domestic Thai and the overall South East Asian market alike.</p>
        
                                </div>
        
        <div id="showTwo" class="myDiv" style="display: none">
                                <div class="row">
                                <div class="col-sm-6">Promotion Time: </div>
                                <div class="col-sm-6">2 Feb 2020 to 11 March 2021</div>
                                </div>
        
                                <!--caoursel-->
                                <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
                                    <div class="carousel-inner">
                                      <div class="carousel-item active">
                                        <img src="/img/100plus.png" width="200" height="200" class="shadow-1-strong mb-4 mx-auto d-block" alt="100plus1">
                                      </div>
                                      <div class="carousel-item">
                                        <img src="/img/100plus1.png" width="200" height="200" class="shadow-1-strong mb-4 mx-auto d-block" alt="100plus2">
                                      </div>
                                      <div class="carousel-item">
                                        <img src="/img/100plus5.png" width="200" height="200" class="shadow-1-strong mb-4 mx-auto d-block" alt="100plus3">
                                      </div>
                                    </div>
                                    <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
                                      <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                                      <span class="sr-only">Previous</span>
                                    </a>
                                    <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
                                      <span class="carousel-control-next-icon" aria-hidden="true"></span>
                                      <span class="sr-only">Next</span>
                                    </a>
                                  </div>
        
                                  <h5 class="text-center">100 Plus</h5>
                                  <p class="text-center">Peak performance is driven by two things passion and a healthy body. Loaded with glucose and minerals, 100PLUS keeps your body properly hydrated for optimum bodily function and speedy recovery. </p>
        </div>
        
                        <div id="showThree" class="myDiv" style="display: none">
                                <div class="row">
                                <div class="col-sm-6">Promotion Time: </div>
                                <div class="col-sm-6">6 Feb 2020 to 11 March 2021</div>
                                </div>
        
                                <!--caoursel-->
                                <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
                                    <div class="carousel-inner">
                                      <div class="carousel-item active">
                                        <img src="/img/coca.png" width="200" height="200" class="shadow-1-strong mb-4 mx-auto d-block" 
              alt="cocacola1">
                                      </div>
                                      <div class="carousel-item">
                                        <img src="/img/coca5.png" width="200" height="200" class="shadow-1-strong mb-4 mx-auto d-block" 
              alt="cocacola2">
                                      </div>
                                      <div class="carousel-item">
                                        <img src="/img/coca6.png" width="200" height="200" class="shadow-1-strong mb-4 mx-auto d-block" 
                                        alt="cocacola3">
                                      </div>
                                    </div>
                                    <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
                                      <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                                      <span class="sr-only">Previous</span>
                                    </a>
                                    <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
                                      <span class="carousel-control-next-icon" aria-hidden="true"></span>
                                      <span class="sr-only">Next</span>
                                    </a>
                                  </div>
        
                                  <h5 class="text-center">Coca Cola Myanmar</h5>
                                  <p class="text-center">Peak performance is driven by two things passion and a healthy body. 100PLUS keeps your body properly hydrated for optimum bodily function and speedy recovery. </p>
                                
                                </div>
        
        
                            <div class="col-md-12 text-center pt-2">
                                <button type="button" id="p2-PrevBtn" class="btn btn-outline-primary" onclick="demoVisibilitytwoPre()"> Previous </button>
                                <button type="button" id="P2-NextBtn" class="btn btn-outline-primary" onclick="demoVisibilitytwoNext()"> Next </button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            </div>


            <!--third page-->
            <div class="container w-75 pt-4 col-sm-6 col-lg-4" id="three" style=" display : none">

                <div class="form-group">
                    <label for="luckydrawcode">Lucky Draw Code</label>
                    <input type="text" class="form-control" name="luckydraw" id="luckydrawcode" placeholder="Enter Your Lucky Draw Code">
                </div>

                <div class="col-md-12 text-center">
                    <button type="button" id="P3-PrevBtn" class="btn btn-outline-primary" onclick="demoVisibilitythreePre()"> Previous  </button>
                    <button type="submit" id="SubmitBtn" class="btn btn-outline-primary"> Submit </button>
                </div>

            </div>
        </form>

        <script src="/js/multi-select"></script>
        
        
    </body>

</html>