                                  <div class="card-body">
                                      <!-- Form subscription -->
                                      <form>
                                          <h3 class="text-center indigo-text font-bold py-4">
                                              <strong>Fill Your Information</strong></h3>
                                          <div class="md-form">
                                              <i class="fa fa-user prefix blue-text"></i>
                                              <input type="text" id="form-name" class="form-control">
                                              <label for="form-name">Your name</label>
                                          </div>
                                          <div class="md-form">
                                              <i class="fa fa-envelope prefix blue-text"></i>
                                              <input type="text" id="form-email" class="form-control">
                                              <label for="form-email">Your email</label>
                                          </div>
                                          <div class="md-form">
                                              <i class="fa fa-pagelines prefix blue-text"></i>  
                                              <input type="text" id="form-age" class="form-control" >
                                              <label for="for-mage">Your Age</label>  
                                          </div>
                                          <div class="md-form">
                                              <i class="fa fa-phone prefix blue-text"></i> 
                                              <input type="tel" class="form-control" id="phonenumber">
                                              <label for="for-mage">Your PhoneNumber</label> 
                                          </div>
          
                                          <div class="md-form">
                                              
                                              <select class="form-select form-select-md mb-3 btn btn-primary" aria-label=".form-select-md example">
                                                  <option selected>Open this select menu</option>
                                                  <option value="1">One</option>
                                                  <option value="2">Two</option>
                                                  <option value="3">Three</option>
                                                </select>
                                               
                                             
                                                <select class="form-select btn btn-primary mb-3 ">
                                                    <option disabled selected>Select Township</option>
                                                    <option>oneome</option>
                                                    <option>oneome</option>
                                                </select>
                                                
                                            </div>
          
          
                                          <div class="text-center py-4">
                                              <button class="btn btn-primary">Send <i class="fa fa-paper-plane-o ml-1"></i></button>
                                          </div>
                                      </form>
                                      <!-- Form subscription -->
                                  </div>
                              </div>
                          </div>
                        
                  </div>
               
                
              </main>


                <div class="col-md-12 text-center">
                    <button type="button" class="btn btn-outline-primary" id="nextBtn1" onclick='demoVisibilityone()'> Next  </button>
                </div>

            </div>

